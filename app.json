        "foregroundImage": "./assets/images/caremed.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.medicare.mobile"
    },
    "web": {
      "favicon": "./assets/images/caremed.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.526614041029-htkjvnkmm8sr62qqd2dkoi2mpbp5epmv"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4b5d8fd8-4c50-4d0e-9f86-73651cedb8a0"
