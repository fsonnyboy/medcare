{
  "expo": {
    "name": "Caremeds",
    "slug": "caremeds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],  
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.medicare.mobile"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.526614041029-htkjvnkmm8sr62qqd2dkoi2mpbp5epmv"
        }
      ]
    ],
    "scheme": "caremeds",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5846c193-c7bd-4d46-839c-79d0c0e10b85"
      }
    }
  }
}
